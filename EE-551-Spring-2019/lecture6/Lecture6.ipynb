      "Enter your input: stop\n"
     ]
    }
   ],
   "source": [
    "# print low if num < 20\n",
    "while True:\n",
    "    reply = input(\"Enter your input: \")\n",
    "    if reply == 'stop': \n",
    "        break\n",
    "    elif not reply.isdigit():\n",
    "        print('Bad bad bad!!!')\n",
    "    else:\n",
    "        num = int(reply) ** 2\n",
    "        if num < 20:\n",
    "            print('low')\n",
    "        else:\n",
    "            print(num)"
